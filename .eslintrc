{
    "extends": "airbnb",
    "rules": {
      "func-names": ["error", "never"],
      "strict": 0,
      "no-param-reassign": ["error", {"props": false}],
      "class-methods-use-this": 0,
      "space-before-function-paren": 0,
      "no-trailing-spaces": 0
    },
    "globals": {
      "use": true,
      "Logger": true
    }
  }
  